{
    "extends": "next/core-web-vitals",
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "webextensions": true
    },
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "comma-dangle": "error",
        "indent":"error"
    }
}