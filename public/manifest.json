{
  "manifest_version": 3,
  "name": "resume-filler",
  "version": "1.0.0",
  "description": "",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "get-dom-content.js"
      ]
    }
  ]
}